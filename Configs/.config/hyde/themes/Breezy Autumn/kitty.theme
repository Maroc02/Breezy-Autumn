$HOME/.config/kitty/theme.conf|killall -SIGUSR1 kitty

# General
foreground #C2C2B0
background #222222

# Black
color0  #222222
color8  #666666

# Red
color1  #685742
color9  #685742

# Green
color2  #5F875F
color10 #5F875F

# Yellow
color3  #B36D43
color11 #B36D43

# Blue
color4  #78824B
color12 #78824B

# Magenta
color5  #BB7744
color13 #BB7744

# Cyan
color6  #C9A554
color14 #C9A554

# White
color7  #D7C483
color15 #D7C483

# Cursor
cursor #FF7979
cursor_text_color #1B1E28

# Selection highlight
